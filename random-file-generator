			</script><script>
				var NoRepeatFilename = (function() {
					var CLS = function(){
						this.values = [];
					}
					CLS.prototype.nuevo = function(nombre){
						var counter = 0;
						var output;
						do {
							counter++;
							output = this.generate(nombre, counter);
						} while(this.exists(output));
						this.values.push(output);
						return output;
					};
					CLS.prototype.generate = function(nombre, i){
						if(i == 1){
							return nombre;
						} else {
							var num = "(" + i + ")";
							var splitted = this.split_fn(nombre);
							return splitted[0] + num + splitted[1];
						}
					};
					CLS.prototype.exists = function(nombre){
						return this.values.indexOf(nombre) != -1;
					};
					CLS.prototype.split_fn = function(name){
						var pos = name.lastIndexOf(".");
						if(pos == -1 || pos == 0) { // file, or .file
							return [name, ""];
						} else {
							return [name.substring(0, pos), name.substring(pos)];
						}
					}
					return CLS;
				})();
				function setFiles(id, files){ // files: Array({name: String, getter: call_without_args -> Promise})
					var get_button_from_file = function(file_info, is_download_all){
						var file_name = file_info.name;
						var file_getter = file_info.getter;
						var button_elem = $("<button>");
						var button_set_class = function(cls){ button_elem.removeClass("state-1 state-2 state-3"); button_elem.addClass(cls); };
						var button_state_1 = function(){ button_set_class("state-1"); };
						var button_state_2 = function(){ button_set_class("state-2"); button_elem.attr("disabled", "disabled"); };
						var button_state_3 = function(){ button_set_class("state-3"); button_elem.removeAttr("disabled"); };
						var button_img = $("<span>");
						var button_text = document.createTextNode(" " + file_name);
						button_elem.append(button_img);
						button_elem.append(button_text);
						button_elem.click(function(){
							button_state_2();
							file_getter().then(function(v) {
								button_state_3();
								var datetime = (new Date((new Date()).getTime() - ((new Date()).getTimezoneOffset() * 60000)))
													.toISOString().substring(0, 19)
													.replace(":", "h").replace(":", "m").replace("T", "_")+"s";
								download(v, is_download_all ? "PineTools.com_" + datetime + ".zip" : file_name);
							});
						});
						button_state_1();
						return button_elem;
					};
					var lista = $("#" + id + " ul");
					lista.empty();
					files.map(function(file_info){
						var button_elem = get_button_from_file(file_info, false);
						var li_elem = $("<li>");
						button_elem.appendTo(li_elem);
						li_elem.appendTo(lista);
					});
					if (files.length > 1) { // two or more files, offer zipped file
						var zipped_getter = function(){
							var zip = new JSZip();
							var folder = zip.folder("PineTools.com_files");
							var no_repeat = new NoRepeatFilename();
							for(var i = 0; i < files.length; i++){
								var name = no_repeat.nuevo(files[i].name);
								var file_promise = files[i].getter(); // <- it is called, generate the promise
								folder.file(name, file_promise, {binary: true});
							}
							return zip.generateAsync({type : "blob"}); // it is a promise
						};
						file_info = { name: 'Download .zip file', getter: zipped_getter };
						var button_elem = get_button_from_file(file_info, true);
						var container = $("#" + id + " .all-zipped");
						container.empty();
						button_elem.appendTo(container);
						// show
						$("#" + id + " .only-multifile").show();
					} else {
						$("#" + id + " .only-multifile").hide();
					}
					window.redrawMasonry(); // NO BORRAR!
				}
